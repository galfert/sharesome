{
  "name": "sharesome",
  "version": "1.0.2",
  "description": "Share files from your remote storage",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean:dist && ember build",
    "build:dist": "npm run clean:dist && ember build -prod && bash scripts/update-versionfile",
    "deploy": "bash scripts/deploy",
